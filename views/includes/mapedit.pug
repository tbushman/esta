fieldset.lcontrol(style="padding:0;border:none;")
	legend(style="font-size: 11px;") layers
	a.drop(v-for="(lyr, o) in layers", style="display: flex;")
		.ui(style="pointer-events: none;min-width:45px;min-width:var(--thumbw);margin:0;")
			//- (style="position:absolute;")
			//- label(:for="lyr._id", v-text="lyr.properties.label", style="font-size: 7px;line-height:7px;height:7px;pointer-events:none;position:absolute;background-color:var(--primary);") 

			img(:src="!lyr.properties.media[lyr.properties.media.length - 1] || !lyr.properties.media[lyr.properties.media.length - 1].thumb || lyr.properties.media[lyr.properties.media.length - 1].thumb === '' ? lyr.properties.media[0].thumb : lyr.properties.media[lyr.properties.media.length - 1].thumb ", style="pointer-events: none;")
		
		span.tb-font-m(v-text="lyr.properties.label", style="width:auto; padding-right: 15px; padding-left: 8px;")
		input(type="checkbox", :value="lyr._id", :id="'lyr_'+o", name="layers[]", v-text="lyr.properties.label", style="position:absolute;", @click="changeLayers(lyr._id, $event)")
		input(type="hidden", :value="JSON.stringify(doc.properties.layers)", name="layers")
		
	legend(style="font-size: 11px;") census
	//- include menu/jurisdiction.pug
	label(for="placetype") Type of jurisdiction
	select#placetype(style="pointer-events: all;", name="placetype",@change="changeCensusJurisdiction($event)")
		option(value="null", v-text="'None'", v-bind:selected="!censusLoad.placetype")
		option(v-for="(pl, p) in placetypes", v-text="pl.name", :value="p")
	label(for="placetype", v-if="censusLoad.cats && censusLoad.cats.length") Data category
	select#place( v-if="censusLoad.cats && censusLoad.cats.length", style="pointer-events: all;", @change="changeCensusData($event)", v-model="censusLoad.cat")
		option(value="null", v-text="'None'", v-bind:selected="!censusLoad.cat")
		option(v-if="censusLoad.cats.length", v-for="(cat, i) in censusLoad.cats", v-text="cat", :value="i", :selected="censusLoad.cat === i")

